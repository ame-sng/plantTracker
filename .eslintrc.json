{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prefer-destructuring": "off",
        "quotes": ["error", "double"],
        "no-multiple-empty-lines": "off",
        "dot-notation": "off"
    }
}
